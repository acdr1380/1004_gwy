<div class="layout">
    <div class="header">
        <div class="header-item">
            <label>选择表册：</label>
            <nz-select [ngModel]="conditionsIfy.value">
                <nz-option
                    *ngFor="let item of conditionsIfy.list"
                    [nzLabel]="item.label"
                    [nzValue]="item.value"
                >
                </nz-option>
            </nz-select>
        </div>
        <div class="header-item">
            <button nz-button nzType="default">开始统计</button>
        </div>
    </div>
    <div class="container"></div>
</div>
