@import 'src/app/styles/layout';
@import 'src/app/styles/drawer';

.layout {
    & > .header {
        height: $header-height;
        line-height: $header-height;
        background-color: #f0f2f5;
        padding-bottom: 5px;

        display: flex;
        align-items: center;
        justify-content: space-between;
        .trans_step {
            min-width: 800px;
        }
    }
    & > .footer {
        button {
            margin-right: 10px;
        }
    }
}
