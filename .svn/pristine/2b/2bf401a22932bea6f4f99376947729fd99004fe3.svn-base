<div class="layout">
    <div class="container">
        <div class="right">
            <div class="layout">
                <div class="header">
                    <div class="right">
                        <nz-button-group>
                            <button nz-button>进入信息</button>
                            <button nz-button>退出信息</button>
                        </nz-button-group>
                    </div>
                </div>
                <div class="container">
                    <div class="view">
                        <excel-control permission="gsb_editor" [params]="URLParams"></excel-control>
                    </div>
                </div>
            </div>
        </div>
        <div class="left person_other_info">
            <div class="layout">
                <div class="header">现职人员</div>
                <div class="container">
                    <div class="view_scroll">
                        <!-- 其它人员信息 -->
                        <table class="person_field_tbl">
                            <tbody>
                                <ng-container *ngFor="let item of personOtherIfy.list">
                                    <tr>
                                        <td class="title">
                                            {{ item.FIELD_EDIT_DISPLAY_NAME + ':' }}
                                        </td>
                                        <td class="value">
                                            <ng-container
                                                *ngIf="
                                                    item.TABLE_COLUMN_DICTIONARY_CODE;
                                                    else notCodeTemp
                                                "
                                            >
                                                {{
                                                    personOtherIfy.data[
                                                        item.TABLE_COLUMN_CODE + '_CN'
                                                    ]
                                                }}
                                            </ng-container>
                                            <ng-template #notCodeTemp>
                                                {{ personOtherIfy.data[item.TABLE_COLUMN_CODE] }}
                                            </ng-template>
                                        </td>
                                    </tr>
                                </ng-container>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
