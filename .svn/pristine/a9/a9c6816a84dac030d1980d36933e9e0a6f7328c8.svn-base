/*导入布局样式*/
@import 'src/app/styles/layout';
@import 'src/app/styles/drawer';

.dictionary {
    height: auto;

    .putt {
        background-color: #fdffdc;

        &[disabled] {
            background-color: #f5f5f5;
        }
    }
}

.dic_selector {
    .container {
        padding: 5px;
    }

    .dictionary_tree {
        height: 100%;
        width: 100%;
    }

    .footer {
        .right {
            float: right;
        }
    }
}
