<div class="layout">
    <div class="header">
        <div class="haeder-item">
            <label>方案类别：</label>
            <dictionary-input code="ZJA" [(text)]="zh_CN.B0604_CN"></dictionary-input>
        </div>
        <div class="haeder-item">
            <label>岗位类别：</label>
            <dictionary-input
                code="ZJB"
                [(text)]="zh_CN.B0605_CN"
            ></dictionary-input>
        </div>
        <div class="right">
            <button nz-button nzType="primary">开始查询</button>
        </div>
    </div>
    <div class="container view_scroll">
        <div class="div-flex">
            <div class="div-flex-item">
                <div class="header">
                    <b>单位基本信息</b>
                </div>
                <div class="content">
                    <ng-template [ngTemplateOutlet]="UnitInfo" ></ng-template>
                </div>
            </div>
            <div class="div-flex-item">
                <div class="header">
                    <b>职数核定信息</b>
                </div>
                <div class="content">
                    <ng-template [ngTemplateOutlet]="ZSInfo"></ng-template>
                </div>
            </div>
            <div class="div-flex-item">
                <div class="header">
                    <b>职数使用信息</b>
                </div>
                <div class="content">
                    <ng-template [ngTemplateOutlet]="ZSUseInfo"></ng-template>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- 单位信息 -->
<ng-template #UnitInfo>
    <table class="const_table">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <col width="12.5%">
        <tbody>
            <tr>
                <td class="title">单位名称</td>
                <td></td>
                <td class="title">单位规格</td>
                <td></td>
                <td class="title">单位性质</td>
                <td></td>
                <td class="title">隶属关系</td>
                <td></td>
            </tr>
            <tr>
                <td class="title">行政编制数</td>
                <td></td>
                <td class="title">参公事业编制数</td>
                <td></td>
                <td class="title">政法专项编制</td>
                <td></td>
                <td class="title">核准基数</td>
                <td></td>
            </tr>
        </tbody>
    </table>
</ng-template>

<!-- 职数核定信息 -->
<ng-template #ZSInfo>
    <table class="const_table">
        <col width="6.25%">
        <col width="6.25%">
