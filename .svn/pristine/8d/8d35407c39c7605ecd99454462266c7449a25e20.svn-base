/*导入布局样式*/
@import 'src/app/styles/_layout';
@import 'src/app/styles/_drawer';

.full_calender {
    padding: 10px;
}

.oper_table {
    padding: 10px;
}

:host ::ng-deep .ant-picker-panel {
    .ant-picker-content {
        tr {
            td,
            th {
                border-right: 1px solid #eee;
                border-bottom: 1px solid #eee;

                &:last-child {
                    border-right: none;
                }
            }
        }
    }
}
