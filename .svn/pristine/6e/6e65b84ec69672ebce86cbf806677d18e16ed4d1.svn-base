/*导入布局样式*/
@import 'src/app/styles/layout';
@import 'src/app/styles/drawer';

.dic_selector {
    border-top: 1px solid #eee;
}

.checked_list {
    width: 200px !important;

    .item {
        width: 100%;
        height: 30px;
        line-height: 30px;
        border-bottom: 1px solid #eee;
        padding: 0 5px;

        .close {
            margin-right: 5px;
        }
        .label {
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
    }
}
