/**
 * 字段类型状态
 */
export enum ColumnTypeEnum {
    /**
     * 主键ID类型
     */
    ID,
    /**
     * 不可变字符串
     */
    CHAR,
    /**
     * 可变字符串
     */
    VARCHAR,
    /**
     * 数字型
     */
    NUMBER,
    /**
     * 时间日期
     */
    DATE,
    /**
     * 是否
     */
    BOOLEAN,
    /**
     * 备注
     */
    CLOB
}

